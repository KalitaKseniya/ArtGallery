@model List<ArtGallery.Models.ArtMovement>

<div class="container row p-0 m-0">
    <div class="col-10">
        <h2 class="text-primary">Добро пожаловать на страницу направлений</h2>
    </div>
</div>
@if (Model.Count() > 0)
{

    @foreach (var art in @Model)
    {
        <p>@art.Name</p>
    }
}
else
{
    <p>Нет направлений</p>
}    